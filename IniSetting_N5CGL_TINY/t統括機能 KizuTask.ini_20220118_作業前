#=========================================================
# 対カメラリンクスイッチ通信
#
# ### 共通仕様
#  TIMER_CYCLE		：定周期チェックタイマ周期 [秒]
#
# ### 個別仕様
# [タスク名_スイッチFPGAボードID]
#  FPGADIE_TEMP_LIMIT	：FPGAダイ温度上限 [℃]
#  FPGADIE_TEMP_ID	：FPGAダイ温度 ID
#  FPGADIE_TEMP_ST_ID	：FPGAダイ温度状態 ID
#  AMBIENT_TEMP_LIMIT	：周囲温度上限 [℃]
#  AMBIENT_TEMP_ID	：周囲温度 ID
#  AMBIENT_TEMP_ST_ID	：周囲温度状態 ID
#  OPTCLINKx_B_ST_ID	：Opt-C:Link B   ステータス ID (x:1〜4) (※ スイッチFPGAマスタならCH1〜4、スイッチFPGAスレーブならCH5〜6に相当)
#  OPTCLINKx_MF_ST_ID	：Opt-C:Link M/F ステータス ID (x:1〜4) (※ スイッチFPGAマスタならCH1〜4、スイッチFPGAスレーブならCH5〜6に相当)
#=========================================================
#=========================================================
#  ※ メールスロットLineIDで、Unitを切替。(0:全部  1〜:対象ユニットのみ)
#=========================================================
[TO_LINKSW_MAIN]

# カメラコントローラ複数台対応
#  NAME_*		：セッション名となる (インスタンスの名前にもなる) MAX8コ
NAME_1 = TO_LINKSW

[TO_LINKSW]
com=COM3
#baud=115200
baud=9600
parity=n
data=8
stop=1
to=off
dtr=off
rts=off
idsr=off
octs=off

# 定周期チェックタイマ周期 [秒]
TIMER_CYCLE = 180

# 高速通信(115200bps)（有効=1。未定義時、無効）
ENABLE_SPEED_CHANGE = 0

# ハーフ版カメラリンクSWフラグ
#--------------------------------------
# HALF_FLG_x = a
#--------------------------------------
# x … ユニット番号(1オリジン、最大 NUM_CAMLINKSW)
# a … 0: フル版（デフォルト）、1:ハーフ版
HALF_FLG_1 = 1
HALF_FLG_2 = 1
HALF_FLG_3 = 1
HALF_FLG_4 = 1

# 各ボードのカメラ入力CH
# テスト的に画像パラ撮りする時などに使用
#--------------------------------------
# FPGA_x_BOARD_y = a, b
#--------------------------------------
# x … ボードID(0オリジン)
# y … 出力ch(1オリジン)
# a … 選択カメラ(-8 〜 8)
#      0 = 無視
#       1〜 8 = Masterとして選択
#      -1〜-8 = Slaveとして選択
# b … 表裏区分(0:表, 1:裏, 2：パララン１、3：パララン2)

